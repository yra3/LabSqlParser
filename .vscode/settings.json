{
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[ignore]": {
		"files.insertFinalNewline": false
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[sln]": {
		"files.encoding": "utf8bom"
	},
	"[sql]": {
		"editor.defaultFormatter": "adpyke.vscode-sql-formatter"
	},
	"[xml]": {
		"editor.defaultFormatter": "qub.qub-xml-vscode"
	},
	"csharp.suppressHiddenDiagnostics": false,
	"debug.onTaskErrors": "showErrors",
	"debug.terminal.clearBeforeReusing": true,
	"dotnet-test-explorer.autoExpandTree": true,
	"dotnet-test-explorer.enableTelemetry": false,
	"dotnet-test-explorer.treeMode": "full",
	"dotnet.enableTelemetry": false,
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"files.associations": {
		"*.dbs": "ignore"
	},
	"files.autoGuessEncoding": false,
	"files.encoding": "utf8",
	"files.eol": "\n",
	"files.exclude": {
		"**/*.bak": true,
		"**/*.user": true,
		"**/.vs": true,
		"**/bin": true,
		"**/obj": true
	},
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"omnisharp.enableEditorConfigSupport": true,
	"omnisharp.enableRoslynAnalyzers": true,
	"omnisharp.organizeImportsOnFormat": true,
	"omnisharp.projectLoadTimeout": 300,
	"qub-xml-vscode.telemetry.enabled": false,
	"terminal.integrated.detectLocale": "off"
}
